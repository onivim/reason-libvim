{
  "name": "reason-libvim",
  "version": "8.10869.6001",
  "description": "Reason adapter for libvim",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "esy": {
    "build": ["refmterr dune build -p libvim"],
    "buildsInSource": "_build",
    "install": [
        "esy-installer libvim.install"
    ]
  },
  "resolutions": {
    "@esy-ocaml/esy-installer": "0.0.1",
    "@opam/dune": "1.7.0"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": ">=4.4.0",
    "libvim": "8.10869.6"
  },
  "devDependencies": {
    "ocaml": ">=4.4.0"
  }
}
